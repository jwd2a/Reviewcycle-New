{
  "name": "@reviewcycle/client",
  "version": "0.1.0",
  "description": "Lightweight prototype review and commenting client",
  "main": "./dist/reviewcycle.umd.js",
  "module": "./dist/reviewcycle.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/reviewcycle.es.js",
      "require": "./dist/reviewcycle.umd.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "review",
    "comments",
    "prototype",
    "feedback",
    "collaboration"
  ],
  "author": "Madera Labs",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jwd2a/Reviewcycle-New"
  },
  "homepage": "https://github.com/jwd2a/Reviewcycle-New#readme",
  "bugs": {
    "url": "https://github.com/jwd2a/Reviewcycle-New/issues"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.8",
    "vite-plugin-dts": "^3.6.4"
  },
  "dependencies": {
    "@paralleldrive/cuid2": "^2.2.2",
    "@reviewcycle/shared": "workspace:*",
    "preact": "^10.19.3"
  }
